<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Home">

  <TextView
      android:id="@+id/home_email"
      android:layout_width="313dp"
      android:layout_height="19dp"
      android:ems="10"
      android:hint="Email"
      android:inputType="textPersonName"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.811"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.022" />

  <Button
      android:id="@+id/btn_home_close"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginTop="8dp"
      android:text="Cerrar"
      app:layout_constraintEnd_toEndOf="@+id/home_email"
      app:layout_constraintHorizontal_bias="1.0"
      app:layout_constraintStart_toStartOf="@+id/home_email"
      app:layout_constraintTop_toBottomOf="@+id/home_email" />

  <TextView
      android:id="@+id/view_instrument"
      android:layout_width="302dp"
      android:layout_height="424dp"
      android:text="TextView"
      tools:layout_editor_absoluteX="54dp"
      tools:layout_editor_absoluteY="153dp" />

</androidx.constraintlayout.widget.ConstraintLayout>